# WiFi/Bluetooth MAC addresses
/(vendor|system/vendor)/bin/hwaddrs2                                u:object_r:hwaddrs2_exec:s0
/(vendor|system/vendor)/bin/hwaddrs.readmisc                        u:object_r:hwaddrs_readmisc_exec:s0

# WLAN
/persist/\.macaddr                                                  u:object_r:mnt_vendor_file:s0

# Bluetooth
/persist/\.baddr                                                    u:object_r:mnt_vendor_file:s0
